{
    "name": "diseltoofast/ue-parser",
    "description": "PHP library for parsing Unreal Engine files",
    "type": "library",
    "license": "MIT",
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Diseltoofast\\UeParser\\": [
                "src/",
                "test/"
            ]
        }
    },
    "require": {
        "php": "^8.0",
        "ext-gmp": "*",
        "diseltoofast/php-ninja": "*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "dev-master"
    }
}
